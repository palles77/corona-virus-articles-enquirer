    "metadata": {
        "title": "Characteristics of Traveler with Middle East Respiratory Syndrome, China, 2015",
        "authors": [
            {
                "first": "Z",
                "middle": [
                    "L"
                ],
                "last": "Chen",
                "suffix": "",
                "affiliation": {},
                "email": ""
            },
