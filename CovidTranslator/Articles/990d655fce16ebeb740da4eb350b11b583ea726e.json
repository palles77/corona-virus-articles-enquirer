    "metadata": {
        "title": "Early biomarkers of inflammation in dogs and cats: the acute phase proteins",
        "authors": [
            {
                "first": "S",
                "middle": [],
                "last": "Paltrinieri",
                "suffix": "",
                "affiliation": {
                    "laboratory": "",
                    "institution": "Universita`di Milano",
                    "location": {
                        "addrLine": "Via Celoria 10",
                        "postCode": "20133",
