                },
                {
                    "start": 1149,
                    "end": 1152,
                    "text": "[4]",
                    "ref_id": "BIBREF3"
                },
                {
                    "start": 1307,
                    "end": 1310,
                    "text": "[3]",
                    "ref_id": "BIBREF2"
                },
                {
                    "start": 1387,
